<configuration>
  <http-servers xsi:schemaLocation="http://marklogic.com/xdmp/group group.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://marklogic.com/xdmp/group">
    <http-server>
      <http-server-name>8071-validator</http-server-name>
      <port>8071</port>
      <modules name="validator-modules"/>
      <database name="validator"/>
      <authentication>application-level</authentication>
      <default-user name="validator-nobody"/>
      <compute-content-length>false</compute-content-length>
      <url-rewriter>/rewriter.xqy</url-rewriter>
    </http-server>
    <http-server>
      <http-server-name>8072-validator-webdav</http-server-name>
      <port>8072</port>
      <webDAV>true</webDAV>
      <execute>false</execute>
      <modules name="validator"/>
      <default-user name="admin"/>
    </http-server>
    <http-server>
      <http-server-name>8073-validator-modules-webdav</http-server-name>
      <port>8073</port>
      <webDAV>true</webDAV>
      <execute>false</execute>
      <modules name="validator-modules"/>
      <authentication>application-level</authentication>
      <default-user name="validator-module-deployer"/>
    </http-server>
    <http-server>
      <http-server-name>8074-validator-schemas-webdav</http-server-name>
      <port>8074</port>
      <webDAV>true</webDAV>
      <execute>false</execute>
      <modules name="validator-schemas"/>
      <authentication>application-level</authentication>
      <default-user name="v-user"/>
    </http-server>
  </http-servers>
  <xdbc-servers xsi:schemaLocation="http://marklogic.com/xdmp/group group.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://marklogic.com/xdmp/group">
    <xdbc-server>
      <xdbc-server-name>8070-validator-xdbc</xdbc-server-name>
      <port>8070</port>
      <modules name="validator-modules"/>
      <database name="validator"/>
      <authentication>basic</authentication>
    </xdbc-server>
    <xdbc-server>
      <xdbc-server-name>8075-validator-modules-xdbc</xdbc-server-name>
      <port>8075</port>
      <database name="validator-modules"/>
    </xdbc-server>
  </xdbc-servers>
  <assignments xsi:schemaLocation="http://marklogic.com/xdmp/assignments assignments.xsd" xmlns="http://marklogic.com/xdmp/assignments" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <assignment>
      <forest-name>validator_forest</forest-name>
    </assignment>
    <assignment>
      <forest-name>validator-modules</forest-name>
    </assignment>
    <assignment>
      <forest-name>validator-schemas</forest-name>
    </assignment>
    <assignment>
      <forest-name>validator-triggers</forest-name>
    </assignment>
  </assignments>
  <databases xsi:schemaLocation="http://marklogic.com/xdmp/database database.xsd" xmlns="http://marklogic.com/xdmp/database" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <database>
      <database-name>validator</database-name>
      <schema-database name="validator-schemas"/>
      <triggers-database name="validator-triggers"/>
      <forests>
  <forest-id name="validator_forest"/>
      </forests>
      <word-searches>true</word-searches>
      <three-character-searches>true</three-character-searches>
      <three-character-word-positions>true</three-character-word-positions>
      <fast-element-character-searches>true</fast-element-character-searches>
      <trailing-wildcard-searches>true</trailing-wildcard-searches>
      <trailing-wildcard-word-positions>true</trailing-wildcard-word-positions>
      <fast-element-trailing-wildcard-searches>true</fast-element-trailing-wildcard-searches>
      <word-lexicons>
  <word-lexicon>http://marklogic.com/collation/codepoint</word-lexicon>
      </word-lexicons>
      <uri-lexicon>true</uri-lexicon>
      <collection-lexicon>true</collection-lexicon>
      <range-element-indexes>
				<range-element-index>
					<scalar-type>int</scalar-type>
					<namespace-uri></namespace-uri>
					<localname>assert-count warning-count error-count job-count warnings-count errors-count
						infos-count</localname>
					<range-value-positions>false</range-value-positions>
				</range-element-index>
				<range-element-index>
					<scalar-type>string</scalar-type>
					<namespace-uri></namespace-uri>
					<localname>vendor-id schema-id project-id job-id</localname>
					<collation>http://marklogic.com/collation/codepoint</collation>
					<range-value-positions>false</range-value-positions>
				</range-element-index>
				<range-element-index>
					<scalar-type>dateTime</scalar-type>
					<namespace-uri></namespace-uri>
					<localname>created</localname>
					<range-value-positions>false</range-value-positions>
				</range-element-index>
			   <range-element-index>
					<scalar-type>int</scalar-type>
					<namespace-uri></namespace-uri>
					<localname>created-year created-month</localname>
					<range-value-positions>false</range-value-positions>
				</range-element-index>
		</range-element-indexes>
			<range-element-attribute-indexes>
				<range-element-attribute-index>
					<scalar-type>string</scalar-type>
					<parent-namespace-uri></parent-namespace-uri>
					<parent-localname>warn error info report</parent-localname>
					<namespace-uri></namespace-uri>
					<localname>id</localname>
					<collation>http://marklogic.com/collation/codepoint</collation>
					<range-value-positions>false</range-value-positions>
				</range-element-attribute-index>
				<range-element-attribute-index>
					<scalar-type>int</scalar-type>
					<parent-namespace-uri></parent-namespace-uri>
					<parent-localname>created</parent-localname>
					<namespace-uri></namespace-uri>
					<localname>year month</localname>
					<range-value-positions>false</range-value-positions>
				</range-element-attribute-index>
				<range-element-attribute-index>
					<scalar-type>dateTime</scalar-type>
					<parent-namespace-uri></parent-namespace-uri>
					<parent-localname>jobdetail job</parent-localname>
					<namespace-uri></namespace-uri>
					<localname>created</localname>
					<range-value-positions>false</range-value-positions>
				</range-element-attribute-index>
			</range-element-attribute-indexes>
    </database>
    <database>
      <database-name>validator-modules</database-name>
      <forests>
  <forest-id name="validator-modules"/>
      </forests>
      <uri-lexicon>true</uri-lexicon>
    </database>
    <database>
      <database-name>validator-triggers</database-name>
      <forests>
